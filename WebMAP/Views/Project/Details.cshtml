<html lang="en">

<!-- begin::Head -->
<head>
    <meta charset="utf-8" />
    <title>Metronic | Invoice v1</title>
    <meta name="description" content="Latest updates and statistic charts">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no">
    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
			WebFont.load({
            google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
          });
    </script>

    <!--end::Web font -->
    <!--begin::Global Theme Styles -->
    <link href="~/Content/assets/vendors/base/vendors.bundle.css" rel="stylesheet" type="text/css" />

    <!--RTL version:<link href="~/Content/assets/vendors/base/vendors.bundle.rtl.css" rel="stylesheet" type="text/css" />-->
    <link href="~/Content/assets/demo/default/base/style.bundle.css" rel="stylesheet" type="text/css" />

    <!--RTL version:<link href="~/Content/assets/demo/default/base/style.bundle.rtl.css" rel="stylesheet" type="text/css" />-->
    <!--end::Global Theme Styles -->
    <link rel="shortcut icon" href="~/Content/assets/demo/default/media/img/logo/favicon.ico" />
</head>

<!-- end::Head -->
<!-- begin::Body -->
<body >
    <!-- begin:: Page -->
    <div class="m-grid m-grid--hor m-grid--root m-page">
        <!-- BEGIN: Header -->
        <!-- END: Header -->
        <!-- begin::Body -->
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
            <!-- BEGIN: Left Aside -->
            <!-- END: Left Aside -->
            <div class="m-grid__item m-grid__item--fluid m-wrapper" style="width:1500px;">

                <!-- END: Subheader -->
                <div class="m-content">
                    <div class="m-portlet">
                        <div class="m-portlet__body m-portlet__body--no-padding">
                            <div class="m-invoice-1">
                                <div class="m-invoice__wrapper">
                                    <div class="m-invoice__head" style="background: url(../../Content/image.jpg) no-repeat center;background-size:1000px;">
                                        <div class="m-invoice__container m-invoice__container--centered">
                                            <div class="m-invoice__logo">
                                                <a href="#">
                                                    <h1></h1>
                                                </a>
                                                <a href="#">
                                                    <img src="~/Content/ezgif.com-gif-maker.png">
                                                </a>
                                            </div>
                                            <span class="m-invoice__desc">

                                                <span>1015 Avenue Wilfrid-Pelletier, Québec </span>
                                                <span>QC G1W 0C4, Canada</span>
                                            </span>
                                            <div class="m-invoice__items">
                                                <div class="m-invoice__item">
                                                    <span class="m-invoice__su  btitle">CREATION DATE</span>
                                                    <span class="m-invoice__text">@ViewBag.result.organigramDate</span>
                                                </div>
                                                <div class="m-invoice__item">
                                                    <span class="m-invoice__subtitle">PROJECT: </span>
                                                    <span class="m-invoice__text">NO. @ViewBag.result.idOrganigram</span>
                                                </div>
                                                <div class="m-invoice__item">
                                                    <span class="m-invoice__subtitle">Contact.</span>
                                                    <span class="m-invoice__text">WWW.LEVIO.CA<br>+1 418-914-3623</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="m-invoice__body m-invoice__body--centered">
                                        <div class="table-responsive">
                                            <table class="table">
                                                <thead>
                                                    <tr>
                                                        <th>COLLABORATOR</th>
                                                        <th></th>
                                                        <th>NAME</th>
                                                        <th></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr>
                                                        <td>Assignment Manager</td>
                                                        <td></td>
                                                        <td>@ViewBag.result.financialManager</td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <td>Financial Manager</td>
                                                        <td></td>
                                                        <td>@ViewBag.result.assignmentManager</td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <td>Project Manager</td>
                                                        <td></td>
                                                        <td>@ViewBag.result.projectManagerName</td>
                                                        <td></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="m-invoice__footer">
                                        <div class="m-invoice__container m-invoice__container--centered">
                                            <div class="m-invoice__content">
                                                <span>CLIENT INFOS.</span>
                                                <span><span>Client Name:</span><span>@ViewBag.result.project.client.clientName</span></span>
                                                <span><span>Phone Number:</span><span>@ViewBag.result.project.client.phoneNumber</span></span>
                                                <span><span>Client Email</span><span>@ViewBag.result.project.client.email</span></span>

                                            </div>
                                            <div class="m-invoice__content">
                                                <img  style="width:100px;margin-left:400px;" src="@Url.Content("~/Content/ProfilePictures/"+ViewBag.result.project.client.logo)" class="m--img-rounded m--marginless" alt="" />
                                                <span></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- end:: Body -->
        <!-- begin::Footer -->
        <!-- end::Footer -->
    </div>

    <!-- end:: Page -->
    <!-- begin::Quick Sidebar -->
    <!-- end::Quick Sidebar -->
    <!-- begin::Scroll Top -->
    <!-- end::Scroll Top -->
    <!-- begin::Quick Nav -->
    <!-- begin::Quick Nav -->
    <!--begin::Global Theme Bundle -->
    <script src="~/Content/assets/vendors/base/vendors.bundle.js" type="text/javascript"></script>
    <script src="~/Content/assets/demo/default/base/scripts.bundle.js" type="text/javascript"></script>

    <!--end::Global Theme Bundle -->
</body>

<!-- end::Body -->
</html>