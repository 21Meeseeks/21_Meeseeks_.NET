
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-xl-6">
        @foreach (var item in ViewBag.projects1)
        {


        <!--begin::Portlet-->
        <div class="m-portlet m-portlet--tabs">
            <div class="m-portlet__head">
                <div class="m-portlet__head-caption">
                    <div class="m-portlet__head-title">
                        <h3 class="m-portlet__head-text">
                            @item.programName
                        </h3>
                    </div>
                </div>
                <div class="m-portlet__head-tools">
                    <ul class="nav nav-tabs m-tabs m-tabs-line   m-tabs-line--right m-tabs-line-danger" role="tablist">
                        <li class="nav-item m-tabs__item">
                            <a class="nav-link m-tabs__link active" data-toggle="tab" href="#m_portlet_tab_1_1" role="tab">
                                Details
                            </a>
                        </li>
                        <li class="nav-item m-tabs__item">
                            <a class="nav-link m-tabs__link" data-toggle="tab" href="#m_portlet_tab_1_2" role="tab">
                                Client
                            </a>
                        </li>
                        <li class="nav-item m-tabs__item">
                            <a class="nav-link m-tabs__link" data-toggle="tab" href="#m_portlet_tab_1_3" role="tab">
                               Organigram
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="m-portlet__body">
                <div class="tab-content">
                    <div class="tab-pane active" id="m_portlet_tab_1_1">
                        
                        @item.project.description

                    </div>
                    <div class="tab-pane" id="m_portlet_tab_1_2">
                    <strong> Name :</strong> @item.project.client.clientName

                    </div>
                    <div class="tab-pane" id="m_portlet_tab_1_3">
                        Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled. Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been
                        the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled.
                    </div>
                </div>
            </div>
        </div>

        <!--end::Portlet-->
        }
    </div>




    <div class="col-xl-6">
        @foreach (var item in ViewBag.projects2)
        {


        <!--begin::Portlet-->
            <div class="m-portlet m-portlet--tabs">
                <div class="m-portlet__head">
                    <div class="m-portlet__head-caption">
                        <div class="m-portlet__head-title">
                            <h3 class="m-portlet__head-text">
                               @item.programName
                            </h3>
                        </div>
                    </div>
                    <div class="m-portlet__head-tools">
                        <ul class="nav nav-tabs m-tabs m-tabs-line   m-tabs-line--right m-tabs-line-danger" role="tablist">
                            <li class="nav-item m-tabs__item">
                                <a class="nav-link m-tabs__link active" data-toggle="tab" href="#m_portlet_tab_1_1" role="tab">
                                    Details
                                </a>
                            </li>
                            <li class="nav-item m-tabs__item">
                                <a class="nav-link m-tabs__link" data-toggle="tab" href="#m_portlet_tab_1_2" role="tab">
                                    Client
                                </a>
                            </li>
                            <li class="nav-item m-tabs__item">
                                <a class="nav-link m-tabs__link" data-toggle="tab" href="#m_portlet_tab_1_3" role="tab">
                                   Organigram
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="m-portlet__body">
                    <div class="tab-content">
                        <div class="tab-pane active" id="m_portlet_tab_1_1">
                            @item.project.description    
                        
                         </div>
                        <div class="tab-pane" id="m_portlet_tab_1_2">


                            <div class="m-card-user m-card-user--skin-dark">
                                <div class="m-card-user__pic">

                                    @if (@item.project.client.logo != null)
                                    {
                                        <img src="@Url.Content("~/Content/ProfilePictures/"+@item.project.client.logo)" class="m--img-rounded m--marginless" alt="" />

                                    }
                                    else
                                    {
                                        <img src="~/Content/assets/app/media/img/users/user4.jpg" class="m--img-rounded m--marginless" alt="" />

                                    }

                                    <!--
            <span class="m-type m-type--lg m--bg-danger"><span class="m--font-light">S<span><span>
            -->
                                </div>
                                <div class="m-card-user__details">
                                    <span  style="color:black" class="m-card-user__name m--font-weight-500">@item.project.client.clientName</span>
                                    <a style="color:black" href="" class="m-card-user__email m--font-weight-300 m-link">@item.project.client.email</a>
                                </div>
                            </div>







  </div>
                        <div class="tab-pane" id="m_portlet_tab_1_3">
                    
                        
                            <div id="m_tree_1" class="tree-demo">
                                <ul>
                                    <li>
                                        Root node 1
                                        <ul>
                                            <li data-jstree='{ "selected" : true }'>
                                                <a href="javascript:;">
                                                    Initially selected
                                                </a>
                                            </li>
                                            <li data-jstree='{ "icon" : "fa fa-briefcase m--font-success " }'>
                                                custom icon URL
                                            </li>
                                            <li data-jstree='{ "opened" : true }'>
                                                initially open
                                                <ul>
                                                    <li data-jstree='{ "disabled" : true }'>
                                                        Disabled Node
                                                    </li>
                                                    <li data-jstree='{ "type" : "file" }'>
                                                        Another node
                                                    </li>
                                                </ul>
                                            </li>
                                            <li data-jstree='{ "opened" : false }'>
                                                @item.project.client.clientName
                                                <ul>
                                                    <li data-jstree='{ "icon" : "fa fa-briefcase m--font-success " }'>
                                                       Project Manager
                                                        <ul>
                                                            <li>@item.projectManagerName</li>

                                                        </ul>
                                                    </li>
                                                    <li data-jstree='{ "icon" : "fa fa-code-branch m--font-success " }'>
                                                        @item.assignmentManager
                                                    </li>
                                                    <li data-jstree='{ "icon" : "fa  fa-dollar-sign  m--font-success " }'>
                                                        @item.financialManager
                                                    </li>
                                                    <li data-jstree=''>
                                                        <ul>
                                                            <li data-jstree='{ "icon" : "fa fa-briefcase m--font-success " }'>
                                                                @item.projectManagerName
                                                            </li>
                                                            <li data-jstree='{ "icon" : "fa fa-code-branch m--font-success " }'>
                                                                @item.assignmentManager
                                                            </li>
                                                            <li data-jstree='{ "icon" : "fa  fa-dollar-sign  m--font-success " }'>
                                                                @item.financialManager
                                                            </li>
                                                            <li data-jstree='{ "type" : "file" }'>
                                                                Another node
                                                            </li>
                                                        </ul>                                                    </li>
                                                </ul>
                                            </li>
                                            <li data-jstree='{ "icon" : "fa fa-warning m--font-danger" }'>
                                                Custom icon class (bootstrap)
                                            </li>
                                        </ul>
                                    </li>
                                    <li data-jstree='{ "type" : "file" }'>
                                        <a href="http://www.keenthemes.com">
                                            Clickanle link node
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            </div>
                    </div>
                </div>
            </div>

        <!--end::Portlet-->
        }
    </div>






</div>


